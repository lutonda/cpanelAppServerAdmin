{% extends 'base.html.twig' %}

{% block body %}
    <div id="wrapper">
        <div id="container">
            <div class="row">
                <div class="col-md-4">Name </div>
                <div class="col-md-4">Type</div>
                <div class="col-md-2">Value</div>
                <div class="col-md-2">Status</div>
                <div class="col-md-4"></div>
            </div>
            {% for data in datas %}
            <div class="row">
                <div class="col-md-4">{{ data.name }}</div>
                <div class="col-md-4">{{ data.type }}</div>
                <div class="col-md-2">{{ data.value }}</div>
                <div class="col-md-2">{{ data.status }}</div>
                <div class="col-md-4"></div>
            </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}

{% block stylesheets %}
{% endblock %}
